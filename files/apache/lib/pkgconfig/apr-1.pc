prefix=/home/vagrant/apache
exec_prefix=/home/vagrant/apache
libdir=${prefix}/lib
APR_MAJOR_VERSION=1
includedir=/home/vagrant/apache/include

Name: APR
Description: The Apache Portable Runtime library
Version: 1.6.2
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -lrt -lcrypt  -lpthread -ldl
Cflags: -DLINUX -D_REENTRANT -D_GNU_SOURCE -g -O2 -pthread -I${includedir}
