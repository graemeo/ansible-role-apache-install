prefix=/home/vagrant/apache
exec_prefix=/home/vagrant/apache
libdir=${prefix}/lib
APRUTIL_MAJOR_VERSION=1
includedir=/home/vagrant/apache/include

Name: APR Utils
Description: Companion library for APR
Version: 1.6.0
# assume that apr-util requires libapr of same major version
Requires: apr-1
Libs: -L${libdir} -laprutil-${APRUTIL_MAJOR_VERSION}  -lexpat
Cflags: -I${includedir}
